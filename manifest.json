{
  "manifest_version": 2,
  "name": "PDFerret for Zotero",
  "version": "1.0.3",
  "description": "Download papers and books by DOI from various providers",
  "homepage_url": "https://github.com/urschrei/pdferret",
  "author": "sth",
  "icons": {
    "48": "skin/default/pdferret-logo.svg",
    "96": "skin/default/pdferret-logo.svg"
  },
  "applications": {
    "zotero": {
      "id": "pdferret@example.com",
      "update_url": "https://github.com/urschrei/pdferret/releases/download/release/updates.json",
      "strict_min_version": "7.0",
      "strict_max_version": "8.*"
    }
  }
}
